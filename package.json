{
  "name": "airbitz-core-js",
  "version": "0.3.1",
  "description": "Airbitz core Javascript implementation",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "homepage": "https://airbitz.co",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-core-js.git"
  },
  "files": [
    "changelog.md",
    "lib/*",
    "LICENSE",
    "package.json",
    "readme.md"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "dependencies": {
    "aes-js": "^3.1.0",
    "base-x": "^1.0.4",
    "disklet": "^0.1.2",
    "elliptic": "^6.4.0",
    "hash.js": "^1.0.3",
    "rfc4648": "^0.9.1",
    "scrypt-js": "^2.0.3",
    "utf8": "^2.1.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "source-map-support": "^0.4.6",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "build": "webpack && rollup -c",
    "format": "prettier --no-semi --single-quote --write '*.js' 'src/**/*.js' 'test/**/*.js' && standard --fix '*.js' 'src/**/*.js' 'test/**/*.js'",
    "lint": "eslint '*.js' 'src/**/*.js' 'test/**/*.js'",
    "precommit": "npm run lint",
    "prepublish": "npm run lint && npm run build",
    "pretest": "npm run lint && webpack && rollup -c rollup.config.test.js",
    "test": "mocha --require source-map-support/register build/tests.cjs.js"
  },
  "browser": {
    "buffer": false
  }
}
