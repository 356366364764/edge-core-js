{
  "name": "airbitz-core-js",
  "version": "0.0.10",
  "description": "Airbitz core Javascript implementation",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "url": "https://airbitz.co",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bin": "bin/airbitz-cli",
  "main": "dist/abc.cjs.js",
  "module": "dist/abc.es6.js",
  "dependencies": {
    "aes-js": "^2.1.0",
    "base-x": "^1.0.4",
    "bip39": "^2.2.0",
    "chalk": "^1.1.3",
    "elliptic": "^6.3.1",
    "hash.js": "^1.0.3",
    "hmac": "^1.0.1",
    "node-getopt": "^0.2.3",
    "node-localstorage": "^1.3.0",
    "scryptsy": "^1.2.1",
    "xmlhttprequest": "^1.4.0"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "rollup": "^0.36.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.5.0"
  },
  "files": [
    "bin/airbitz-cli*",
    "dist/abc.*.js*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-core-js.git"
  },
  "scripts": {
    "build": "rollup -c && rollup -c rollup.config.cli.js",
    "lint": "standard 'src/**/*.js' 'test/**/*.js' cli.js 'cli/**/*.js'",
    "prepublish": "npm run lint && npm run build",
    "pretest": "npm run lint && rollup -c rollup.config.test.js",
    "test": "mocha dist/tests.cjs.js"
  }
