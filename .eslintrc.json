{
  "extends": [
    "standard-kit",
    "standard-kit/node",
    "standard-kit/jsx",
    "standard-kit/flow"
  ],
  "rules": {
    "camelcase": "error",
    "flowtype/generic-spacing": "off",
    "no-throw-literal": "error",
    "no-var": "error",
    "prefer-const": "error",
    "standard/computed-property-even-spacing": "off"
  }
}
